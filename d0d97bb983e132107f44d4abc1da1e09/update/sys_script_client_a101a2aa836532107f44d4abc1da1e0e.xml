<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Battery capacity is to be between 30-200 kwh</description>
        <field>battery_capacity</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Battery Capacity Limit</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    //Type appropriate comment here, and begin script below
	if (newValue < 30 || newValue > 200) {
        g_form.showFieldMsg("battery_capacity", "Invalid battery capacity (valid range 30 - 200 kWh)", "error");
    } else {
        g_form.hideFieldMsg("battery_capacity", true);
    }

}

]]></script>
